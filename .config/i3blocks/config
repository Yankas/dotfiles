
[USD]
command=$HOME/.local/bin/i3sb usd
interval=3600


[WEATHER]
command=$HOME/.local/bin/i3sb weather
interval=600

[TIME]
command=$HOME/.local/bin/i3sb time
interval=1

[DATE]
command=$HOME/.local/bin/i3sb date
interval=60

[VOLUME]
command=$HOME/.local/bin/i3sb volume
interval=3

[UPDATES]
command=$HOME/.local/bin/i3sb updates
interval=3
